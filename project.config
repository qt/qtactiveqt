[project]
	description = The Qt ActiveQt repository\n\
\n\
ssh://codereview.qt-project.org:29418/qt/qtactiveqt.git
[receive]
	requireChangeId = true
	requireContributorAgreement = true
	allowTopicReview = true
[submit]
	action = cherry pick
	mergeContent = true
[cherryPick]
	includeReviewedOn = true
[cherryPick "categoryFooters"]
	Sanity-Review = true
[access "refs/for/refs/heads/release"]
	exclusiveGroupPermissions = push
